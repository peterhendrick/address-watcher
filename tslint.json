{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "semicolon": [
            true,
            "always"
        ],
        "trailing-comma": [false],
        "curly": [false],
        "max-line-length": [true, 200]
    },
    "rulesDirectory": []
}